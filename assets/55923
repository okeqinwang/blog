    <script nonce="6BUHAAD0ZT">
        $(function () {
            //自定义js
            $("[name='unhelpful_mark']").bind('click',function(){
                var obj = $(this);
                var user_unhelpful_mark = $(this).attr("user_unhelpful_mark");//已经标注无帮助是1 未标注是0
                var aid = $(this).attr('aid');
                if(user_unhelpful_mark == 1){//当前已标记没有帮助，点击取消标记
                    var method = "/yqapi/ask/unmarkAnswer/"+aid;
                }else{//当前未标记没有帮助，点击了标记无帮助
                    var method = "/yqapi/ask/markAnswer/"+aid;
                }

                $.ajax({
                    type: 'post',
                    url: method,
                    async: false,
                    dataType:"json", 
                    data: {},
                    success: function(data, status, xhr) {
                        console.log(data);
                        if(data.errcode){//返回错误
                            $.alert(data.errmsg);
                        }else{
                            if(user_unhelpful_mark == 1){//当前已标记没有帮助，点击取消打标
                                obj.attr("user_unhelpful_mark",0);
                                obj.text('没有帮助');
                            }else{//当前未打标，点击了没有帮助
                                obj.attr("user_unhelpful_mark",1);
                                obj.text('撤销没有帮助');
                            }
                        }             
                    },
                    error: function(xhr, type, error) {
                        console.log(error.toString());
                    }
                });
            });
            // 收藏的代理处理
            $(".icon-collection").click(function () {
                $("#question_mark_btn").click();
            })
            if (window.location.hash == '#answer') {
                $("html, body").animate({ scrollTop: $('.list-content').offset().top }, 100);
            }
        });
    </script>
    <script nonce="6BUHAAD0ZT">
