<script nonce="6BUHAAD0ZT">
    $(document).on('ajax:complete', ['form.js-comment-create', 'form.sub-comment-form'], function (ev) {
        var nowHrefs = {};
        $(ev.target).parents('.sub-comments-box').find('a.js-comment-destroy').each(function () {
            var href = $(this).attr('href');
            if (nowHrefs.hasOwnProperty(href)) {
                $(this).parents('.sub-comment').remove();
            }
            nowHrefs[href] = href;
        });
        $(document).off('ajax:complete', ['form.js-comment-create', 'form.sub-comment-form']);
    });
</script>
