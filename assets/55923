<link href="/mditor/css/mditor.min.css?v=20171127" rel="stylesheet">
<script src="/mditor/js/mditor.min.js?v=20171127"></script>

<script type="text/javascript"
        src="//yqfile.alicdn.com/mathjax/2.7.2/MathJax.js?config=TeX-AMS-MML_HTMLorMML"></script>
<script type="text/x-mathjax-config" nonce="6BUHAAD0ZT">
              MathJax.Hub.Config({
                tex2jax: {inlineMath: [['$','$'], ['\\(','\\)']]},
                jax: ["input/TeX","output/HTML-CSS"],
              });
        </script>
<script nonce="6BUHAAD0ZT">
    var obj = {};
    var moditor = function (id, height) {

        obj['mditor-'+id] = Mditor.fromTextarea(document.getElementById("markdown-edit-textarea-"+id));

        obj['mditor-'+id].on('ready', function () {
            obj['mditor-'+id].split = false; //关闭分屏
            obj['mditor-'+id].toolbar.removeItem('toggleSplit');//删除预览按钮
            //跟换预览按钮原来的逻辑

            obj['mditor-'+id].toolbar.getItem('togglePreview').icon = 'eye';

            obj['mditor-'+id].toolbar.getItem('togglePreview').handler = function(){
                let _this = this;
                $.ajax({
                    type: "POST",
                    url:'/yqapi/articles/preview',
                    data:{draft:obj['mditor-'+id].value},
                    dataType: "json",
                    success: function(data){
                        $('markdown-edit-textarea-').html(data.data.content);
                        _this.preview = !_this.preview

                    }
                })
            };
            setTimeout(function () {
                $('[data-cmd="togglePreview"]').html(' 预览').css('width','auto');
                $('[data-cmd="image"]').html('');
            },100);

            obj['mditor-'+id].height = height+'px';
            //关闭帮助btn
            // var helpBtn = mditor.toolbar.getItem("help");
            // helpBtn.handler = function () {
            //     window.open('https://yq.aliyun.com/help/markdown', '_blank');
            // };
            obj['mditor-'+id].toolbar.removeItem('help');
            obj['mditor-'+id].toolbar.getItem('image');

            var upload = function (file) {
                if (file == null) {
                    return;
                }
                var name = file.name || 'screenshot.png';
                name = name.replace(/\.(?:jpg|gif|png)$/i, ''); // clear ext
                name = name.replace(/\W+/g, '_'); // clear unvalid chars

                var data = new FormData();
                data.append('fileToUpload', file, name);
                $.yqTooltipShow('<div><img src="https://yqfile.alicdn.com/0e6e0f46d7504242302bc8055ad9c8c2.gif" style="width: 35px;padding-bottom: 10px;"><p style="width: 400px;font-size: 18px;">正在上传图片 ...</p></div>');
                $.ajax('/upload/img2', {
                    data: data,
                    type: 'POST',
                    processData: false,
                    contentType: false
                })
                    .done(function (res) {
                        $.yqTooltipHide();
                        obj['mditor-'+id].editor.insertBeforeText('![' + name + '](' + res + ")\n");
                    })
                    .fail(function ($xhr) {
                        if ($xhr.responseText) {
                            $.alert($xhr.responseText);
                        }
                    });
            };

            obj['mditor-'+id].toolbar.getItem('image').handler = function () {
                var accept = {
                    image: 'image/png, image/gif, image/jpg, image/jpeg',
                };
                var $file = $('<input type="file" accept="' + accept.image + '">');
                $file.click();
                $file.on('change', function () {
                    var file = this.files[0];
                    upload(file);
                });
            };

            obj['mditor-'+id].editor.on('paste', function (evt) {
                var data = evt.clipboardData;
                if (typeof data !== 'object') {
                    return;
                }

                return $.each(data.items, function (i, item) {
                    if (item.kind === 'file' && item.type.indexOf('image/') !== -1) {
                        var blob = item.getAsFile();
                        upload(blob);
                    }
                });
            });
            obj['mditor-'+id].editor.on('drop', function (evt) {
                evt.preventDefault();
                return $.each(evt.dataTransfer.files, function (i, file) {
                    return upload(file);
                });
            });

            var mathJaxTimer = null;
            obj['mditor-'+id].editor.on('changed', function (evt) {
                if (mathJaxTimer) {
                    clearTimeout(mathJaxTimer);
                    mathJaxTimer = null;
                }

                mathJaxTimer = setTimeout(function () {
                    MathJax.Hub.Queue(["Typeset", MathJax.Hub]);
                }, 200);
            });

            obj['mditor-'+id].toolbar.items.splice(10,1,
                {
                    name: 'table',
                    icon: 'table',
                    handler: function () {
                        obj['mditor-'+id].editor.wrapSelectText('标题1',' | 标题2 | 标题3  \n' +
                            '------- | ------- | -------  \n' +
                            '文本1 | 文本2 | 文本3  ');
                    },
                    title: '表格',
                    key: 'shift+alt+t'
                }
            );
            // 居中
            obj['mditor-'+id].toolbar.items.splice(6,0,
                {
                    name: 'align-center',
                    icon: 'align-center',
                    handler: function () {
                        obj['mditor-'+id].editor.wrapSelectText('<p style="text-align:center">', '</p>');
                    },
                    title: '居中',
                    key: ''
                }
            );

            obj['mditor-'+id].toolbar.items.splice(14,0,
                {
                    name: 'file-video',
                    icon: 'file-video-o',
                    handler: function () {
                        obj['mditor-'+id].editor.wrapSelectText('<iframe style="text-align:center;border:0;" width="100%" height="400px" src="','"></iframe>');
                    },
                    title: '视频',
                    key: '',
                }
            );

            var isMac = function() { return /macintosh|mac os x/i.test(navigator.userAgent); }();

            obj['mditor-'+id].toolbar.items.splice(0,0,{
                name: 'mail-reply',
                icon: 'mail-reply',
                handler: function () {
                    obj['mditor-'+id].editor.undo();
                },
                title: '撤销',
                key: 'ctrl+z'
            },{
                name: 'mail-forward',
                icon: 'mail-forward',
                handler: function () {
                    obj['mditor-'+id].editor.redo();
                },
                title: '恢复',
                key: isMac ? 'command+shift+z' : 'ctrl+y',
            });

            setTimeout(function () {
                MathJax.Hub.Queue(["Typeset", MathJax.Hub]);
            }, 100);

            // mathJax 前端渲染过滤
            var mathJaxContainer = {};
            mathJaxContainer.ele = [];

            mathJaxContainer.input = function (match) {
                mathJaxContainer.ele.push(match);
                return "{mathJaxContainer["+ (mathJaxContainer.ele.length-1) +"]}";
            };

            mathJaxContainer.output = function (match,i) {
                return mathJaxContainer.ele[i];
            };

            obj['mditor-'+id].viewer.$on('beforeRender',function (event) {
                if(!event.value)return;
                event.value = event.value.replace(/(\${1,2})(.+?)\1/g,mathJaxContainer.input);
                event.value = event.value.replace(/(\${2})[\S\s]*?\1/mg,mathJaxContainer.input);
            });

            obj['mditor-'+id].viewer.$on('afterRender',function (event) {
                if(!event.value)return;
                event.value = event.value.replace(/\{mathJaxContainer\[(\d+)\]\}/g,mathJaxContainer.output);
                mathJaxContainer.ele = [];
            });

            obj['mditor-'+id].viewer.value = obj['mditor-'+id].editor.value;
        });


    }
</script>                                                                                            <div class="a-item" id="answer-96160">
